  toggleMode: () => void;
};

const TopBar: React.FC<TopBarProps> = ({ toggleMode }) => {
  return (
    <AppBar position="static">
      <Toolbar>
        <Typography variant="h6" style={{ flexGrow: 1 }}>
          Todo List
        </Typography>
        <IconButton edge="end" color="inherit" onClick={toggleMode}>
          <Brightness4Icon />
        </IconButton>
      </Toolbar>
    </AppBar>
  );
};

export default TopBar;
```

